{
  "name": "11ty-sass",
  "version": "1.0.0",
  "description": "11ty starter template with SASS support and Dark Mode.",
  "keywords": [
    "11ty",
    "SASS",
    "SCSS",
    "eleventy"
  ],
  "author": "Matt Daniel Brown",
  "license": "ISC",
  "private": true,
  "scripts": {
    "watch:sass": "node-sass --watch src/sass -o src/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "node-sass src/sass -o src/css",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass --parallel build:*"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "bourbon": "^7.2.0",
    "inter-ui": "^3.19.3",
    "mobi.css": "^3.1.1",
    "modern-normalize": "^1.1.0",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "reseter.css": "^2.0.0",
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "postcss": "^8.4.21",
    "sass": "^1.57.1"
  }
}
